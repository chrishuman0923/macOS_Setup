{
  "auto-rename-tag.activationOnLanguage": ["html", "javascriptreact"],
  "color-highlight.markerType": "foreground",
  "color-highlight.markRuler": false,
  "color-highlight.matchWords": true,
  "debug.console.fontFamily": "Anonymous Pro, monospace",
  "debug.console.fontSize": 18,
  "diffEditor.ignoreTrimWhitespace": false,
  "fontshortcuts.defaultFontSize": 20,
  "fontshortcuts.defaultTerminalFontSize": 20,
  "fontshortcuts.step": 2,
  "eslint.alwaysShowStatus": true,
  "editor.detectIndentation": true,
  "editor.fontFamily": "Anonymous Pro, monospace",
  "editor.fontSize": 20,
  "editor.formatOnSave": true,
  "editor.lineHeight": 0,
  "editor.minimap.enabled": false,
  "editor.mouseWheelZoom": true,
  "editor.renameOnType": true,
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "emmet.showAbbreviationSuggestions": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.closeExtensionDetailsOnViewChange": true,
  "extensions.ignoreRecommendations": true,
  "extensions.showRecommendationsOnlyOnDemand": true,
  "files.autoSave": "off",
  "files.enableTrash": false,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "gitlens.defaultDateFormat": "MM/DD/YYYY hh:mm:ss A ZZ",
  "gitlens.defaultDateShortFormat": "MM/DD/YYYY",
  "markdown.preview.fontFamily": "Anonymous Pro, monospace",
  "markdown.preview.fontSize": 20,
  "prettier.arrowParens": "avoid",
  "prettier.bracketSpacing": true,
  "prettier.endOfLine": "lf",
  "prettier.jsxBracketSameLine": false,
  "prettier.jsxSingleQuote": true,
  "prettier.printWidth": 110,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "all",
  "prettier.useTabs": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.confirmOnExit": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "Anonymous Pro, monospace",
  "terminal.integrated.fontSize": 20,
  "window.zoomLevel": 0,
  "workbench.colorTheme": "One Dark Pro",
  "workbench.editor.enablePreview": false,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 5,
  "workbench.editor.openPositioning": "last",
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tips.enabled": false,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.tree.indent": 16,
  "[*]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
